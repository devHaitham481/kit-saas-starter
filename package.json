{
  "name": "kit-saas-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "paraglide-js compile --project ./project.inlang && vite build",
    "preview": "pnpm run build && wrangler pages dev .svelte-kit/cloudflare",
    "test": "pnpm run test:integration && pnpm run test:unit",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "test:integration": "playwright test",
    "test:unit": "vitest",
    "build-cf-types": "pnpm exec wrangler types && mv worker-configuration.d.ts src/",
    "drizzle:migrate": "drizzle-kit generate:sqlite",
    "drizzle:push:dev": "pnpm wrangler d1 migrations apply kss-prod --local",
    "drizzle:push:prod": "pnpm wrangler d1 migrations apply kss-prod",
    "postinstall": "paraglide-js compile --project ./project.inlang",
    "wrangler:db:prod:backup": "pnpm wrangler d1 export kss-prod --output backup-prod.sql --remote",
    "inlang:lint": "pnpx @inlang/cli lint --project project.inlang"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240405.0",
    "@inlang/paraglide-js": "1.6.0",
    "@inlang/paraglide-js-adapter-sveltekit": "^0.6.4",
    "@playwright/test": "^1.43.0",
    "@sveltejs/adapter-cloudflare": "^4.3.0",
    "@sveltejs/kit": "^2.5.5",
    "@sveltejs/vite-plugin-svelte": "^3.1.0",
    "@total-typescript/ts-reset": "^0.5.1",
    "@types/eslint": "8.56.8",
    "typescript-eslint": "7.6.0",
    "autoprefixer": "^10.4.19",
    "drizzle-kit": "^0.20.14",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "2.37.0",
    "globals": "^15.0.0",
    "pino-pretty": "^11.0.0",
    "postcss": "^8.4.38",
    "postcss-load-config": "^5.0.3",
    "prettier": "^3.2.5",
    "prettier-plugin-svelte": "^3.2.2",
    "prettier-plugin-tailwindcss": "^0.5.13",
    "svelte": "5.0.0-next.100",
    "svelte-check": "^3.6.9",
    "svelte-turnstile": "^0.5.0",
    "sveltekit-flash-message": "^2.4.4",
    "sveltekit-rate-limiter": "^0.5.1",
    "sveltekit-view-transition": "^0.5.3",
    "tailwindcss": "^3.4.3",
    "tslib": "^2.6.2",
    "typescript": "^5.4.5",
    "vite": "^5.2.8",
    "vite-plugin-kit-routes": "^0.6.1",
    "vite-plugin-top-level-await": "^1.4.1",
    "vite-plugin-wasm": "^3.3.0",
    "vitest": "^1.5.0",
    "wrangler": "^3.50.0"
  },
  "type": "module",
  "dependencies": {
    "@lucia-auth/adapter-sqlite": "^3.0.1",
    "arctic": "^1.5.0",
    "bits-ui": "^0.21.3",
    "check-password-strength": "^2.0.10",
    "clsx": "^2.1.0",
    "drizzle-orm": "^0.30.8",
    "formsnap": "^1.0.0",
    "lucia": "^3.1.1",
    "lucide-svelte": "^0.367.0",
    "mode-watcher": "^0.3.0",
    "oslo": "^1.2.0",
    "pino": "^8.20.0",
    "svelte-sonner": "^0.3.22",
    "sveltekit-superforms": "^2.12.4",
    "tailwind-merge": "^2.2.2",
    "tailwind-variants": "^0.2.1",
    "worker-password-auth": "^1.0.0",
    "zod": "^3.22.4"
  }
}
